	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#7f8c98"
			}
		}, {
			"name": "Preprocessor Statements",
			"scope": [
				"meta.preprocessor",
				"keyword.control.import"
			],
			"settings": {
				"foreground": "#f0a55f"
			}
		}, {
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#ff8270"
			}
		}, {
			"name": "Template string expressions",
			"scope": [
				"meta.template.expression",
				"keyword.operator.assignment"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		}, {
			"name": "Template string interpolation",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#9664c8"
			}
		}, {
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#d9c97c"
			}
		}, {
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"foreground": "#f082b0",
				"fontStyle": "bold"
			}
		}, {
			"name": "User-defined constant",
			"scope": [
				"constant.character"
			],
			"settings": {
				"foreground": "#64afc8"
			}
		}, {
			"name": "Variable",
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#64b4a0"
			}
		}, {
			"name": "Variable property",
			"scope": "variable.other.property",
			"settings": {
				"foreground": "#a0f0dc"
			}
		}, {
			"name": "Keyword",
			"scope": [
				"keyword"
			],
			"settings": {
				"foreground": "#f082b0",
				"fontStyle": "bold"
			}
		}, {
			"name": "Operator",
			"scope": [
				"keyword.operator",
				"keyword.operator.arithmetic",
				"storage.type.function.arrow"
			],
			"settings": {
				"foreground": "#9664c8"
			}
		}, {
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"foreground": "#f082b0",
				"fontStyle": "bold"
			}
		}, {
			"name": "Types",
			"scope": "support.type.property-name",
			"settings": {
				"foreground": "#87dbfa"
			}
		}, {
			"name": "Class name",
			"scope": [
				"entity.name.type.class"
			],
			"settings": {
				"foreground": "#bef0e6"
			}
		}, {
			"name": "Superclass",
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#87beb4",
				"fontStyle": "bold"
			}
		}, {
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#64afc8"
			}
		}, {
			"name": "Invoked function",
			"scope": "entity.name.function.method",
			"settings": {
				"foreground": "#64b4a0"
			}
		}, {
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#ffffff"
			}
		}, {
			"name": "Reserved keywords",
			"scope": "variable.language",
			"settings": {
				"foreground": "#f082b0",
				"fontStyle": "bold"
			}
		}, {
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#f082b0"
			}
		}, {
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#d9c97c"
			}
		}, {
			"name": "Library function",
			"scope": "support.function",
			"settings": {
				"foreground": "#64b4a0"
			}
		}, {
			"name": "Library constant",
			"scope": "support.constant",
			"settings": {
				"foreground": "#9ef1de"
			}
		}, {
			"name": "Library class/type",
			"scope": [
				"support.type",
				"support.class"
			],
			"settings": {
				"foreground": "#d2befa"
			}
		}, {
			"name": "Library variable",
			"scope": "support.other.variable",
			"settings": {
				"foreground": "#aa82e6"
			}
		}, {
			"name": "Invalid",
			"scope": "invalid",
			"settings": {}
		}, {
			"scope": "token.info-token",
			"settings": {
				"foreground": "#316bcd"
			}
		}, {
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		}, {
			"scope": "token.error-token",
			"settings": {
				"foreground": "#cd3131"
			}
		}, {
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#800080"
			}
		}
	]
}
