{
	"name": "Native macOS - Light Theme, Dark Editor",
	"$schema": "vscode://schemas/color-theme",
	"type": "light",
	"colors": {
		"activityBar.background": "#303136",
		"activityBar.border": "#414045",
		"activityBar.dropBackground": "#555555",
		"activityBar.foreground": "#ffffff",
		"activityBar.inactiveForeground": "#ffffff8f",
		"activityBarBadge.background": "#0a3caa",
		"activityBarBadge.foreground": "#ffffff",
		"badge.background": "#199aff",
		"badge.foreground": "#ffffff",
		"breadcrumb.background": "#ffffff",
		"breadcrumb.foreground": "#000000",
		"button.background": "#696969",
		"button.foreground": "#fcfbfc",
		"button.hoverBackground": "#696969",
		"descriptionForeground": "#000000",
		"dropdown.background": "#000000",
		"dropdown.border": "#ffffff33",
		"dropdown.foreground": "#000000",
		"dropdown.listBackground": "#000000",
		"editor.background": "#282a30",
		"editor.foreground": "#ffffff",
		"editor.inactiveSelectionBackground": "#6e6e6e",
		"editor.lineHighlightBackground": "#2e3239",
		"editor.selectionBackground": "#636f83",
		"editor.selectionHighlightBackground": "#636f8380",
		"editor.wordHighlightBackground": "#636f8380",
		"editor.wordHighlightStrongBackground": "#636f8380",
		"editorCursor.foreground": "#ffffff",
		"editorError.border": "#ff000000",
		"editorError.foreground": "#e21515",
		"editorGroup.border": "#afafaf",
		"editorGroup.dropBackground": "#555555",
		"editorGroup.emptyBackground": "#303136",
		"editorGroup.focusedEmptyBorder": "#00000000",
		"editorGroupHeader.noTabsBackground": "#303136",
		"editorGroupHeader.tabsBackground": "#cdcdcd",
		"editorGroupHeader.tabsBorder": "#1a191c",
		"editorGutter.addedBackground": "#4dbf57",
		"editorGutter.background": "#ff000000",
		"editorGutter.deletedBackground": "#e21515",
		"editorGutter.modifiedBackground": "#348cff",
		"editorHint.border": "#ff000000",
		"editorHint.foreground": "#027aff",
		"editorInfo.border": "#ff000000",
		"editorInfo.foreground": "#4dbf57",
		"editorLineNumber.activeForeground": "#ffffffd8",
		"editorLineNumber.foreground": "#747478",
		"editorOverviewRuler.addedForeground": "#4dbf57",
		"editorOverviewRuler.border": "#ff000000",
		"editorOverviewRuler.bracketMatchForeground": "#636f8380",
		"editorOverviewRuler.deletedForeground": "#e21515",
		"editorOverviewRuler.errorForeground": "#e21515",
		"editorOverviewRuler.findMatchForeground": "#636f8380",
		"editorOverviewRuler.infoForeground": "#027aff",
		"editorOverviewRuler.modifiedForeground": "#027aff",
		"editorOverviewRuler.rangeHighlightForeground": "#636f8380",
		"editorOverviewRuler.selectionHighlightForeground": "#636f8380",
		"editorOverviewRuler.warningForeground": "#ffc501",
		"editorOverviewRuler.wordHighlightForeground": "#636f8380",
		"editorOverviewRuler.wordHighlightStrongForeground": "#636f8380",
		"editorPane.background": "#1e1e1e",
		"editorSuggestWidget.foreground": "#000000",
		"editorUnnecessaryCode.opacity": "#000000c0",
		"editorWarning.border": "#ff000000",
		"editorWarning.foreground": "#ffc501",
		"editorWidget.background": "#303136",
		"editorWidget.foreground": "#000000",
		"errorForeground": "#ece1e0",
		"focusBorder": "#4f4e53",
		"foreground": "#ffffff",
		"input.background": "#414045",
		"input.foreground": "#ffffff",
		"inputOption.activeBackground": "#242529",
		"list.activeSelectionBackground": "#403f44",
		"list.focusBackground": "#403f44",
		"list.inactiveFocusBackground": "#403f44",
		"list.inactiveSelectionBackground": "#403f44",
		"panel.dropBackground": "#555555",
		"panelTitle.activeBorder": "#ff000000",
		"panelTitle.activeForeground": "#ffffff",
		"panelTitle.inactiveForeground": "#ffffff8f",
		"scrollbar.shadow": "#ffffff00",
		"scrollbarSlider.activeBackground": "#ffffff8f",
		"scrollbarSlider.background": "#ffffff1c",
		"scrollbarSlider.hoverBackground": "#ffffff8f",
		"selection.background": "#636f83",
		"sideBar.background": "#303136",
		"sideBar.border": "#1a191c",
		"sideBar.dropBackground": "#555555",
		"sideBar.foreground": "#000000",
		"sideBarSectionHeader.background": "#cdcdcdcc",
		"sideBarSectionHeader.border": "#303136",
		"sideBarSectionHeader.foreground": "#000000",
		"sideBarTitle.foreground": "#6c6b70",
		"statusBar.background": "#414045",
		"statusBar.border": "#4d4d50",
		"statusBar.debuggingBackground": "#414045",
		"statusBar.debuggingBorder": "#4d4d50",
		"statusBar.debuggingForeground": "#a0a0a0",
		"statusBar.foreground": "#a0a0a0",
		"statusBar.noFolderBackground": "#414045",
		"statusBar.noFolderBorder": "#4d4d50",
		"statusBar.noFolderForeground": "#a0a0a0",
		"statusBarItem.activeBackground": "#414045",
		"statusBarItem.hoverBackground": "#414045",
		"symbolIcon.booleanForeground": "#ff7ab3",
		"tab.activeBackground": "#d2d2d2",
		"tab.activeBorder": "#1a191c",
		"tab.activeForeground": "#000000",
		"tab.border": "#a5a5a5",
		"tab.inactiveBackground": "#303136",
		"tab.inactiveForeground": "#b7b7b7",
		"tab.unfocusedActiveBorder": "#00000000",
		"tab.unfocusedActiveForeground": "#ffffff15",
		"textBlockQuote.background": "#ff000000",
		"textBlockQuote.border": "#ff000000",
		"textCodeBlock.background": "#ff000000",
		"textLink.activeForeground": "#6699fe",
		"textLink.foreground": "#6699fe",
		"textPreformat.foreground": "#ffffffd8",
		"textSeparator.foreground": "#ffffffd8",
		"titleBar.activeBackground": "#414045",
		"titleBar.activeForeground": "#b8b7bc",
		"titleBar.border": "#4d4d50",
		"titleBar.inactiveBackground": "#414045",
		"titleBar.inactiveForeground": "#b8b7bc",
		"panel.background": "#242529",
		"panel.border": "#4d4d50",
	},
	"tokenColors": [{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7f8c98"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "#4b555f"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#5a6e78"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"foreground": "#fc5fa3",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Assignment Operators",
			"scope": "keyword.operator.assignment",
			"settings": {
				"foreground": "#d2aaff"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#6bdfff"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"variable.language"
			],
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				"foreground": "#fa5fa0"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
			],
			"settings": {
				"foreground": "#64b4a0"
			}
		},
		{
			"name": "Libraries",
			"scope": "support.constant",
			"settings": {
				"foreground": "#64b4a0"
			}
		},
		{
			"name": "Library Functions",
			"scope": "support.function",
			"settings": {
				"foreground": "#a0f0dc"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"foreground": "#1e464b"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Numbers, Characters",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#d2bf69"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end"
			],
			"settings": {
				"foreground": "#fa6464"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#6f37aa"
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#3d00a0"
			}
		},
		{
			"name": "Strings: Symbols",
			"scope": "constant.other.symbol",
			"settings": {
				"foreground": "#6e32aa"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "HTML: Doctype Declaration",
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype string",
				"meta.tag.sgml.doctype entity.name.tag",
				"meta.tag.sgml punctuation.definition.tag.html"
			],
			"settings": {
				"foreground": "#AAAAAA"
			}
		},
		{
			"name": "HTML: Tags",
			"scope": [
				"meta.tag",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#9b2391"
			}
		},
		{
			"name": "HTML: Tag Names",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#9b2391"
			}
		},
		{
			"name": "HTML: Attribute Names",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#825f05"
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "CSS: Selectors",
			"scope": [
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#7A3E9D"
			}
		},
		{
			"name": "CSS: Property Names",
			"scope": [
				"meta.property-name",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "CSS: Property Values",
			"scope": [
				"meta.property-value",
				"meta.property-value constant.other",
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "CSS: Important Keyword",
			"scope": "keyword.other.important",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Deletion",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: Error",
			"scope": "markup.error",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Markup: Insertion",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Link",
			"scope": "meta.link",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Markup: Output",
			"scope": [
				"markup.output",
				"markup.raw"
			],
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Markup: Prompt",
			"scope": "markup.prompt",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Markup: Heading",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#AA3731"
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Traceback",
			"scope": "markup.traceback",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Markup: Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#7A3E9D"
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#5a6e78"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": "#4b555f"
			}
		},
		{
			"name": "Extra: Diff Range",
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": "meta.diff.header.from-file",
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff To",
			"scope": "meta.diff.header.to-file",
			"settings": {
				"foreground": "#434343"
			}
		}
	]
}
